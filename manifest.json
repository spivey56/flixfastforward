{
    "name": "Flix Fast Forward",
    "description": "Never sit through an intro on netflix again",
    "version": "0.1",
    "permissions": [
    "tabs","<all_urls>"
    ],
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"

    },

    "content_scripts": [
        {
            "matches": [
                "https://www.netflix.com/watch/*"
            ],

            "js": [
                "jquery-1.12.3.min.js",
                "content.js"
            ],
            "run_at": "document_end"
        }
    ],
    "manifest_version":2
}
