{
    "name": "Flix Fast Forward",
    "description": "Never sit through an intro on netflix again",
    "version": "0.1",
    "permissions": [
    "tabs","<all_urls>"
    ],
    "browser_action": {
        "default_icon": "icon.png"
    },

    "content_scripts": [
        {
        "matches": [
        "https://www.netflix.com/watch/*"
            ],
        "js": ["content.js"],
        "run_at": "document_end"         // pay attention to this line
        }
    ], 
    "manifest_version":2
}